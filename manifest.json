{
	"manifest_version": 2,
	"name": "Eng-Vi Dictionary",
	"version" : "1.0.1",
	"icons": { "16": "16x16.png",
           "48": "48x48.png",
          "128": "128x128.png" },
	"background": {
		"scripts": ["jquery-2.1.4.min.js", "background.js"]
	},
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"js" : ["jquery-2.1.4.min.js", "translate.js"],
			"run_at": "document_end"
		}
	],
	"permissions":["http://www.vdict.com/", "https://translate.google.com/*"]

}